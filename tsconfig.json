{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@typing/*": [
        "src/typing/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@icons/*": [
        "src/assets/icons/*"
      ],
      "@stylescomp/*": [
        "src/styles/components/*"
      ],
      "@styleslay/*": [
        "src/styles/layouts/*"
      ],
      "@stylespag/*": [
        "src/styles/pages/*"
      ],
      "@styles": [
        "src/styles/Globals.scss"
      ],
    }
  }
}